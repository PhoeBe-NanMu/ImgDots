<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lnyp.imgdots">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission-sdk-23 android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission-sdk-23 android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.lnyp.imgdots.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.SpacePageActivity"></activity>


        <activity android:name=".activity.PoupWindowDemoActivity"></activity>

        <activity android:name=".activity.ShowPathActivity" />
        <activity android:name=".activity.DetailsActivity" />

        <activity
            android:name="com.lnyp.imgdots.activity.ImageBrowseActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>